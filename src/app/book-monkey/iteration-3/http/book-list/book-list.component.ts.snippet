@Component({
  // ...
})
export class BookListComponent implements OnInit {
  books: Book[];

  constructor(private bs: BookStoreService) { }

  // im HTML:
  // <a *ngFor="let b of books" [book]="b" /*TEX:\bfs*/[routerLink]="b.isbn"/*TEX:\bfe*/></a>
  ngOnInit() {
    this.bs.getAll().subscribe(res => this.books = res);
  }
}
